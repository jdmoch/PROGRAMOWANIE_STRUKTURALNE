    {
        static int liczba=0;
        liczba++;
        printf("Te funckej wywalona %d razy\n",liczba);
    }

int main()
{
    zlicz();
     zlicz();
      zlicz();
       zlicz();
        zlicz();
        return 0;
}

ZADANIE 2 SUMOWANIE

int zsumuj(int a)
    {
        static int liczba=0;
        liczba +=a;
        printf("podana liczba %d i suma wynosi %d \n",a, liczba);
        return liczba;
    }

int main()
{
    zsumuj(5);
    zsumuj(3);
    zsumuj(8);
    zsumuj(2);
    zsumuj(1);
}

