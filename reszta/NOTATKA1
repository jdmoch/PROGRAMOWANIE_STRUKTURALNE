int silnia(int a)
{
      if(a==0)
      {
          return 1;
      }
      else
      {
          return a*silnia(a-1);
      }

}

int main()
{
   printf("%d\n",silnia(0));
   printf("%d\n",silnia(6));
   printf("%d\n",silnia(4));
   printf("%d\n",silnia(1));
}

