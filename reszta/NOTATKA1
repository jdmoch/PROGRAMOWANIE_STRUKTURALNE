  int wyraz(int n)
{
    int a = 1;
    if(n==0||n==1)
    {
        return 1;
    }
    else
    {
        return 2*wyraz(n-1)+5;
    }

}

int main()
{
    int i;
    for(i=0; i<10; i++)
    {
        printf("%d\n",wyraz(i));
    }

}

ZADANIE 5 cos innego
int wyraz(int n)
{
    int a = 1;
    if(n==0||n==1)
    {
        return 1;
    }
    else
    {
        return wyraz(n-1)+2*wyraz(n-2)+3;
    }

}

int main()
{
    int i;
    for(i=0; i<10; i++)
    {
        printf("%d\n",wyraz(i));
    }

}

