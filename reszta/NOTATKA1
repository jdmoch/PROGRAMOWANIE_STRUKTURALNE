int main()
{
    int n,m,k;
    scanf("%d",&n);
    int i;
    int w = 1;
    for(i=2; i<=n; i++)
    {
       w=w*i;
    }
    printf("%d!=%d",n,w);
    return 0;
}

int main()
{
    int n;
    scanf("%d",&n);
    int i;
    int w=0;
    for(i=1; i<=n; i++)
    {
       w=w+i*i;
    }
    printf("1^2+2^2+...+%d^2=%d",n,w);
    return 0;
}

