        printf("%d ",tab1[i]);
    }
    printf("\n");
}
int min(unsigned int n,int * tab1){
    int wynik=tab1[0];
    for(int i=0;i<n;i++){
        if(tab1[i]<wynik){
            wynik=tab1[i];
        }
    }
    return wynik;
}
