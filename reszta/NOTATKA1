//zad 4.2.10
void wypisz(unsigned int n,int * tab1){
    for(int i=0;i<n;i++){
        printf("%d ",tab1[i]);
    }
    printf("\n");
}
int max(unsigned int n,int * tab1){
    int wynik=tab1[0];
    for(int i=0;i<n;i++){
        if(tab1[i]>wynik){
            wynik=tab1[i];
        }
    }
    return wynik;
}
int main()
{
    int tab1[]={2,7,1,3};
    printf("max: %d",max(4,tab1));
}
